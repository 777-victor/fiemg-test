{
  "name": "fiemg-test",
  "version": "1.0.0",
  "main": "src/index.ts",
  "repository": "https://github.com/777-victor/fiemg-test.git",
  "author": "Victor <victorboaventcampos@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "tsc",
    "start": "ts-node -r tsconfig-paths/register src/index.ts",
    "dev": "nodemon --watch 'src/' --exec 'ts-node -r tsconfig-paths/register src/index.ts' -e ts",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo:all --env 'production'",
    "db:migrate": "rm -Rf dist && npm run build && npx sequelize db:migrate",
    "db:seed": "npx sequelize-cli db:seed:all --env 'production'",
    "db:seed:undo": "npx sequelize-cli db:seed:undo",
    "test": "echo \"Jest: running all tests\" && cross-env NODE_ENV=test jest",
    "lint": "eslint --ext .ts src",
    "lint:fix": "eslint --ext .ts src --fix"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.1",
    "@types/cors": "^2.8.15",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/node": "^20.8.10",
    "@types/node-cron": "^3.0.10",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@types/express": "^4.17.20",
    "@types/jest": "^29.5.7",
    "@types/validator": "^13.11.5",
    "axios": "^1.6.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "http-status": "^1.7.3",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.6.3",
    "node-cron": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.34.0",
    "sequelize-cli": "^6.6.2",
    "sequelize-typescript": "^2.1.5",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "winston": "^3.11.0",
    "yup": "^1.3.2"
  }
}
